{
	"compilerOptions": {
		"target": "es2020",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"strict": true,
		"skipLibCheck": true,
		"module": "preserve",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"verbatimModuleSyntax": true,
		"allowJs": true,
		"types": ["@fastly/js-compute"]
	}
}
